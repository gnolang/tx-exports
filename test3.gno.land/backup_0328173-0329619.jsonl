\n\tif nt.Done != newtask.Done {\n\t\tt.Fatalf(\"done is not good\")\n\t}\n\n\t// test add a task\n\tid := tl.Tasks.Size()\n\ttl.AddTask(id, nt)\n\tif tl.Tasks.Size() != 1 {\n\t\tt.Fatalf(\"task not added\")\n\t}\n\n\t// test ToggleTaskStatus\n\ttask, exists := tl.Tasks.Get(\"0\")\n\tif !exists {\n\t\tt.Fatalf(\"task not found\")\n\t}\n\tToggleTaskStatus(task.(*Task))\n\tif task.(*Task).Done != true {\n\t\tt.Fatalf(\"task not toggled\")\n\t}\n}\n"}]},"deposit":"1ugnot"}],"fee":{"gas_wanted":"10000000","gas_fee":"50000ugnot"},"signatures":[{"pub_key":{"@type":"/tm.PubKeySecp256k1","value":"AidlxNTtkl1dDNCgxrSNVJKYWZdC9yrixvgSAP6U6WYX"},"signature":"23bSGagGyEq6zEa8knWtU+kdBfekgu3MX+7pGc+g8ElxRHh7usIqE7IdgDBcPIXHAEjx1/1U4cNVej0Jys3N7w=="}],"memo":"Deployed through play.gno.land"},"blockNum":"328806"}
